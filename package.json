{
  "name": "lindeneg",
  "private": true,
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "scripts": {
    "start": "node ./dist/index.mjs",
    "dev": "npx -y concurrently \"yarn build:main --watch\" \"yarn build:lib --watch\" \"npx -y nodemon ./dist/index.mjs --dev\"",
    "dev:build": "npx -y concurrently \"yarn build:main --watch\" \"yarn build:lib --watch\"",
    "build": "yarn build:main && yarn build:lib",
    "build:main": "rollup -c",
    "build:lib": "tsc -p tsconfig.browser.json",
    "lint": "prettier \"src/**\" --ignore-unknown --write",
    "studio": "npx -y prisma studio",
    "migrate": "npx -y prisma@5.1.1 migrate dev",
    "generate": "npx -y prisma@5.1.1 generate"
  },
  "dependencies": {
    "@lindeneg/funkallero": "0.0.5",
    "@lindeneg/funkallero-auth-service": "0.0.5",
    "@prisma/client": "5.1.1",
    "cloudinary": "1.40.0",
    "cookie-parser": "1.4.6",
    "handlebars": "4.7.8",
    "showdown": "2.1.0",
    "zod": "3.22.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "25.0.4",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.17",
    "@types/handlebars": "4.1.0",
    "@types/node": "20.5.0",
    "@types/showdown": "2.0.1",
    "@types/simplemde": "^1.11.8",
    "prettier": "^3.0.2",
    "rollup": "3.28.0",
    "rollup-plugin-cleaner": "1.0.0",
    "rollup-plugin-typescript2": "0.35.0",
    "typescript": "5.1.6"
  }
}
